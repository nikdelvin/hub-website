---
import { Code } from 'astro:components'

interface Props {
    brand: 'Taily' | 'Cybergelion' | 'Peppermeet'
    color?: string
}

const { brand, color } = Astro.props

function renderBackround(color = 'default') {
    switch (color) {
        case 'default':
            return 'from-indigo-200 via-blue-200 to-sky-200'
        case 'secondary':
            return 'from-purple-500 via-violet-500 to-indigo-500'
        case 'primary':
            return 'from-blue-500 via-sky-500 to-cyan-500'
        case 'success':
            return 'from-teal-500 via-emerald-500 to-green-500'
        case 'warning':
            return 'from-yellow-500 via-amber-500 to-orange-500'
        case 'error':
            return 'from-red-500 via-rose-500 to-pink-500'
        case 'ghost':
            return 'from-zinc-400 via-gray-400 to-slate-400'
    }
}
